{
  "name": "documentviewer",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "build:ngx-doc-viewer": "nx build ngx-doc-viewer --prod",
    "build:react-documents": "rimraf ./dist/libs/react-documents && rollup -c",
    "publish:react-documents": "npm run build:react-documents && npm publish ./dist/libs/react-documents --tag latest",
    "publish:ngx-doc-viewer": "npm run build:ngx-doc-viewer  && npm publish ./dist/libs/ngx-doc-viewer --tag latest",
    "publish-next:ngx-doc-viewer": "npm run build:ngx-doc-viewer && npm publish ./dist/libs/ngx-doc-viewer --tag next",
    "start:demo:angular": "nx serve demo-angular --port=4200",
    "start:demo:react": "nx serve demo-react --port=4201",
    "deploy:demo:angular": "firebase use angular-doc-viewer && firebase deploy --only hosting:demo-angular",
    "deploy:demo:react": "firebase use angular-doc-viewer && firebase deploy --only hosting:demo-react",
    "prepare-build": "node tools/scripts/prepare-build.ts ngx-doc-viewer",
    "lint": "nx workspace-lint && ng lint",
    "format:write": "nx format:write",
    "update": "nx migrate latest",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.0.0",
    "@angular/common": "12.0.0",
    "@angular/compiler": "12.0.0",
    "@angular/core": "12.0.0",
    "@angular/forms": "12.0.0",
    "@angular/platform-browser": "12.0.0",
    "@angular/platform-browser-dynamic": "12.0.0",
    "@angular/router": "12.0.0",
    "@nrwl/angular": "12.5.0-beta.5",
    "core-js": "^3.6.5",
    "document-register-element": "1.13.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "0.11.4"
  },
  "peerDependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.0.0",
    "@angular-eslint/eslint-plugin": "12.0.0",
    "@angular-eslint/eslint-plugin-template": "12.0.0",
    "@angular-eslint/template-parser": "12.0.0",
    "@angular/cli": "12.0.0",
    "@angular/compiler-cli": "12.0.0",
    "@angular/language-service": "12.0.0",
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
